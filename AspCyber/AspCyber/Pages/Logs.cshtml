@page
@using AspCyber.Data.Models;
@model LogsModel
@{
    ViewData["Title"] = "Log List";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Description</th>
            <th>Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in (List<LogViewModel>)ViewData["Logs"])
        {
            <tr>
                <td>@log.UserNameModel</td>
                <td>@log.Description</td>
                <td>@log.Time.ToString("yyyy-MM-dd HH:mm:ss")</td>
            </tr>
        }
    </tbody>
</table>
